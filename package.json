{
  "name": "elysia-clerk",
  "version": "0.2.1",
  "description": "Unofficial Clerk plugin for Elysia.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/elysia-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/elysia-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/elysia-clerk/issues",
  "keywords": [
    "bun",
    "auth"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run --watch ./dev/index.tsx",
    "build": "bun run ./build.mjs",
    "prepublishOnly": "bun run build",
    "release": "bumpp && npm publish",
    "lint": "biome check src",
    "format": "biome format src --write"
  },
  "peerDependencies": {
    "elysia": ">= 0.7.0"
  },
  "dependencies": {
    "@clerk/backend": "^0.31.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.1",
    "@elysiajs/html": "^0.7.3",
    "bumpp": "^9.2.0",
    "bun-plugin-dts-auto": "^0.2.2",
    "bun-types": "^1.0.7",
    "elysia": "^0.7.19",
    "typescript": "5.2.2"
  }
}
